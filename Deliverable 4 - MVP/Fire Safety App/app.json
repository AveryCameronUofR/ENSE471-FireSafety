{
  "expo": {
    "name": "Fire Safety Test App",
    "slug": "firesafety",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "description": "911 Practice Call Simulator: Fire Education App to practice calling 911 for fire emergencies with questions and prompts",
    "githubUrl": "https://github.com/AveryCameronUofR/ENSE471-FireSafety/tree/master/Deliverable%204%20-%20MVP"
